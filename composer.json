{
    "name": "filmoteca/infrastructure",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "filmoteca/filmoteca",
                "version": "dev-ansible",
                "source": {
                    "url": "https://github.com/filmoteca/filmoteca.git",
                    "type": "git",
                    "reference": "ansible"
                }
            }
        }
    ],
    "authors": [
        {
            "name": "Victor Aguilar",
            "email": "victor.aguilar@clickbus.com"
        }
    ],
    "require-dev": {
        "filmoteca/filmoteca": "dev-ansible"
    },
    "scripts": {
        "post-install-cmd": [
            "cp vendor/filmoteca/filmoteca/Vagrantfile ./"
